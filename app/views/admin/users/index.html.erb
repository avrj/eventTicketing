<h1 class="page-header">Admins</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Username</th>
      <th>Level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.username %></td>
        <td><%= admin.level %></td>
        <td><%= link_to 'Show', admin_user_path(admin) %></td>
        <td><%= link_to 'Edit', edit_admin_user_path(admin) %></td>
        <td><%= link_to 'Destroy', admin_user_path(admin), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin', new_admin_user_path %>
